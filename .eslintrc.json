{
  "env": {
    "node": true,
    "browser": true,
    "es6": true,
    "jest": true
  },
  "parser": "babel-eslint",
  "extends": [
    "airbnb",
    "prettier"
  ],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "all",
        "singleQuote": true,
        "printWidth": 100
      }
    ],
    "import/prefer-default-export": 0,
    "react/jsx-filename-extension": 0,
    "no-use-before-define": 0,
    "react/prop-types": 0,
    "react/no-array-index-key": 0,
    "react/jsx-props-no-spreading": 0,
    "no-param-reassign": 0,
    "no-plusplus": 0,
    "no-nested-ternary": 0
  },
  "plugins": [
    "prettier"
  ]
}
